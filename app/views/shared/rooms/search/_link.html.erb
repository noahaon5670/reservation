<% @rooms.each do |room| %>
  <div class="col-sm-6 col-lg-4 col-xl-3">
    <div class="room-flame-inner">
      <%= link_to room do %>
        <div class="room-image-out" >
          <img src=<%= room.room_image %> class="room-image" alt="部屋画像" >
        </div>
        <div class="room-profile">
          <p class="room-profile-name"><%= room.name %></p>
          <p class="room-profile-city"><%= room.address %></p>
          <p class="room-profile-price price-icon"><%= room.fee %></p>
        </div>
      <% end %>
    </div>
  </div>
<% end %>
