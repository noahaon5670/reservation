<% params = {
    room_image: "ルーム画像",
    name: "ルーム名",
    explanation: "ルーム紹介",
    fee: "料金",
    created_at: "作成日",
} %>

<div class="room-posts">
  <h4 class="pb-4">登録済みルーム一覧</h4>
  <table>
    <thead>
      <%= render "shared/list/rooms-list/thead", params: params, isEdit: true%>
    </thead>
    <tbody>
      <% @rooms.each do |room|%>
        <%= render "shared/list/rooms-list/tbody", params: params, model: room, isEdit: true%>
      <% end %>
    </tbody>
  </table>
</div>
